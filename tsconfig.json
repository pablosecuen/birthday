{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/components/NavbarMenu/NavbarMenu.jsx", "app/components/NavbarMenu/NavnarMenuDesktop.jsx", "app/components/Nabvar/Navbar.jsx", "utils/data.js", "utils/emailScript.js", "utils/formValidation.js", "utils/handleNavItemClick.js", "utils/handleSmoothScroll.js", "app/components/Contact/Contact.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
